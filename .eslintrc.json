{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
     "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "no-unused-vars": ["warn"],
    "no-console": ["warn"],
    "react/jsx-fragments": ["error", "syntax"],
    "camelcase": ["error", { "properties": "never" }]
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  }
}
